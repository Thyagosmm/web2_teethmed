<form action="/alterarpaciente" method="POST">
  <input type="hidden" id="cpf" name="cpf" value="<%= paciente.cpf %>">
  <label for="nome">Nome do Paciente:</label><br>
  <input type="text" id="nome" name="nome" value="<%= paciente.nome %>"><br>
  <label for="idade">Idade do Paciente:</label><br>
  <input type="text" id="idade" name="idade" value="<%= paciente.idade %>"><br>
  <input type="submit" value="Salvar Alterações">
</form>